te>\n  <div\n    class=\"number-container d-flex justify-content-center align-items-center\"\n  >\n    <!-- 减 1 的按钮 -->\n    <button type=\"button\" class=\"btn btn-light btn-sm\" @click=\"sub()\">-</button>\n    <!-- 购买的数量 -->\n    <span class=\"number-box\">{{ num }}</span>\n    <!-- 加 1 的按钮 -->\n    <button type=\"button\" class=\"btn btn-light btn-sm\" @click=\"add()\" :id=\"id\">\n      +\n    </button>\n  </div>\n</template>\n\n<script>\nimport bus from \"@/components/eventBus.js\";\nexport default {\n  props: {\n    id: {\n      type: Number,\n      required: true,\n    },\n    num: {\n      default: 1,\n      type: Number,\n    },\n  },\n  methods: {\n    add() {\n      const obj = {\n        id: this.id,\n        value: this.num + 1,\n      };\n      bus.$emit(\"share\", obj);\n    },\n    sub() {\n      const obj = {\n        id: this.id,\n        value: this.num - 1,\n      };\n      if(obj.value>=1){\n        bus.$emit(\"share\", obj);\n      }\n      \n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.number-box {\n  min-width: 30px;\n  text-align: center;\n  margin: 0 5px;\n  font-size: 12px;\n}\n\n.btn-sm {\n  width: 30px;\n}\n</style>\n"],"sourceRoot":"src/components/Counter"}]}